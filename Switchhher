#include <iostream>
using namespace std;	

				

int main(){
	int subjects;
	int grade;
	
		cout << ("Choose Subject: [1]Physics,[2]Chemisty,[3]Biology,[4]Mathematics,[5]Computer: \n");
		cin >> subjects;
		
	switch(subjects){
		case 1:
			cout << "Please Input Physics Grade: ";
			cin >> grade;

			if(grade >= 90){
				cout << "Physics:"<<"Grade A"<< endl;	
		}
			if(grade >= 80){
				cout << "Physics:"<<"Grade B"<< endl;
		}
			if(grade >= 70){
				cout << "Physics:"<<"Grade C"<< endl;
		}
			if(grade >= 60){
				cout << "Physics:"<<"Grade D"<< endl;
		}
			if(grade >= 40){
				cout << "Physics:"<<"Grade E"<< endl;
		}
			if(grade < 40){
				cout << "Physics:"<<"Grade F"<< endl;
		}
			break;
		case 2:
			cout << "Please Input Chemistry Grade: ";
			cin >> grade;
		
		if(grade >= 90){
				cout << "Chemistry:"<<"Grade A"<< endl;	
		}
			if(grade >= 80){
				cout << "Chemistry:"<<"Grade B"<< endl;
		}
			if(grade >= 70){
				cout << "Chemistry:"<<"Grade C"<< endl;
		}
			if(grade >= 60){
				cout << "Chemistry:"<<"Grade D"<< endl;
		}
			if(grade >= 40){
				cout << "Chemistry:"<<"Grade E"<< endl;
		}
			if(grade < 40){
				cout << "Chemistry:"<<"Grade F"<< endl;
		}
			break;
		case 3:
		
			cout << "Please Input Biology Grade: ";
			cin >> grade;
		
		if(grade >= 90){
				cout << "Biology:"<<"Grade A"<< endl;	
		}
			if(grade >= 80){
				cout << "Biology:"<<"Grade B"<< endl;
		}
			if(grade >= 70){
				cout << "Biology:"<<"Grade C"<< endl;
		}
			if(grade >= 60){
				cout << "Biology:"<<"Grade D"<< endl;
		}
			if(grade >= 40){
				cout << "Biology:"<<"Grade E"<< endl;
		}
			if(grade < 40){
				cout << "Biology:"<<"Grade F"<< endl;
		}
			break;
		case 4:
		
			cout << "Please Input Mathematics Grade: ";
			cin >> grade;
		
		if(grade >= 90){
				cout << "Mathematics:"<<"Grade A"<< endl;	
		}
			if(grade >= 80){
				cout << "Mathematics:"<<"Grade B"<< endl;
		}
			if(grade >= 70){
				cout << "Mathematics:"<<"Grade C"<< endl;
		}
			if(grade >= 60){
				cout << "Mathematics:"<<"Grade D"<< endl;
		}
			if(grade >= 40){
				cout << "Mathematics:"<<"Grade E"<< endl;
		}
			if(grade < 40){
				cout << "Mathematics:"<<"Grade F"<< endl;
		}
			break;
		case 5:
			cout << "Please Input Computer Grade: ";
			cin >> grade;
		
		if(grade >= 90){
				cout << "Computer:"<<"Grade A"<< endl;	
		}
			if(grade >= 80){
				cout << "Computer:"<<"Grade B"<< endl;
		}
			if(grade >= 70){
				cout << "Computer:"<<"Grade C"<< endl;
		}
			if(grade >= 60){
				cout << "Computer:"<<"Grade D"<< endl;
		}
			if(grade >= 40){
				cout << "Computer:"<<"Grade E"<< endl;
		}
			if(grade < 40){
				cout << "Computer:"<<"Grade F"<< endl;
		}
			break;
		default:
			printf("Subject Unknown");
			break;
			
}
	return 0;
}
